{
  "name": "danielhusar.sk",
  "description": "My personal site and blog",
  "version": "1.0.0",
  "author": "Daniel Husar <dano.husar@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop --port 8002",
    "start": "gatsby clean && npm run develop",
    "serve": "gatsby serve",
    "lint:tsc": "tsc",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@daniel.husar/gatsby-theme-spring": "*",
    "gatsby": "^2.22.11",
    "gatsby-plugin-manifest": "^2.4.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-social-icons": "^4.1.0"
  },
  "devDependencies": {
    "@daniel.husar/a11y-static-report": "^1.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,mdx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
{
  "scripts": {
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  }
}
